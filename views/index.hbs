<div class="row">
{{#each products}}
<div class="col-4">
<div class="card" style="width: 18rem;">
  <img class="card-img-top" src="{{this.Image}}" alt="Card image cap">
  <div class="card-body">
    <h5 class="card-title">{{this.Name}}</h5>
    <p class="card-text">{{this.Des}}</p>
    <a href="#" class="btn btn-primary">Buy</a>
  </div>
</div>
</div>
{{/each}}
</div>
